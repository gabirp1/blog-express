<%- include('./partials/header.ejs') %>
<main>
    <section>


        <% if (posts.length > 0) { %>
            <div class="blog">
            <% posts.forEach(post => { %>
                <div class="blog">
                    <p class="marca"><%= blog.marca %></p>
                    <p class="modelo"><%= blog.modelo %></p>
                    <a href="<%= "/fullindex/"+post._id %>">View Post</a>
                </div>
            <% }) %>
            </div>


            <% }) %>
            <% } else { %>
            <p>No hay nada que mostrar...</p>
            <% } %>

        </div>


    </section>
</main>
<%- include('./partials/footer.ejs') %>